{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowUnreachableCode": false,
    "noImplicitAny": false,
    "outDir": "built-es5"
  },
   "files": [
      "app-lazy/main.ts",
      "app-lazy/app.module.ts",
      "app-lazy/spreadsheet.module.ts",
      "app-lazy/lazy-loaded-treeview.module.ts"
   ],
   "angularCompilerOptions": {
    "genDir": "./app-lazy/aot",
    "entryModule": "app-lazy/app.module#AppModule"
  }
}