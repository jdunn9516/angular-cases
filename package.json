{
  "name": "angular-samples",
  "scripts": {
    "start-remote-cache": "docker-compose -f caching/docker-compose.yml up",
    "stop-remote-cache": "docker-compose -f caching/docker-compose.yml down",
    "create-vendor-bundle": "webpack --config vendor/webpack-config.js",
    "build-all": "yarn bazel build //src/apps/demo-app:bundle && yarn bazel build //src/apps/api && yarn bazel build //src/apps/backend",
    "start": "yarn build-all && concurrently \"yarn bazel run //src/apps/api\" \"yarn bazel run //src/apps/backend\"",
    "start-watch": "yarn build-all && concurrently \"ibazel build //src/apps/demo-app:bundle\" \"ibazel run //src/apps/api\" \"ibazel run //src/apps/backend\""
  },
  "devDependencies": {
    "@angular/bazel": "7.0.1",
    "@angular/compiler": "7.0.1",
    "@angular/compiler-cli": "7.0.1",
    "@bazel/ibazel": "0.6.0",
    "@bazel/typescript": "0.20.3",
    "@bazel/bazel": "0.18.1",
    "@types/node": "^10.12.0",
    "compression": "1.7.2",
    "concurrently": "3.5.1",
    "path": "0.12.7",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8",
    "typescript": "3.1.1"
  },
  "dependencies": {
    "angular": "~1.4.7",
    "tslib": "1.9.3",
    "bootstrap": "~3.3.4",
    "core-js": "^2.4.1",
    "events": "^1.0.2",
    "express": "4.16.3",
    "fbjs": "^0.3.1",
    "flux": "^2.1.1",
    "jquery": "3.3.1",
    "jquery-ui-dist": "1.12.1",
    "react": "~0.13.3",
    "request": "2.87.0",
    "systemjs": "0.20.19",
    "underscore": "~1.8.3",
    "zone.js": "0.8.26"
  }
}
