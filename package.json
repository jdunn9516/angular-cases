{
  "name": "angular-samples",
  "scripts": {
    "aot-i18n-en": "ngc -p src/tsconfig-i18n.json && rollup -c rollup/rollup-config-i18n.js -o dist/build-en.js",
    "aot-i18n-no": "ngc --i18nFile=./src/apps/i18n-demo-app/locale/messages.no.xlf --locale=no --i18nFormat=xlf -p src/tsconfig-i18n.json && rollup -c rollup/rollup-config-i18n.js -o dist/build-no.js",
    "generate-i18n-xlf": "ng-xi18n -p i18n/tsconfig.json",
    "build-demo-app": "ngc -p src/tsconfig-upgrade.json && rollup -c rollup/rollup-ng-upgrade.js",
    "browserify-demo": "ngc -p src/tsconfig-es5-commonjs-aot.json && browserify built-es5-commonjs-aot/src/apps/bundler-comparison-app/main.js -o dist/browserify-bundle.js && uglifyjs dist/browserify-bundle.js -o dist/browserify-bundle.min.js -c -m",
    "closure-compiler-demo": "ngc -p src/tsconfig-es6.json && java -jar node_modules/google-closure-compiler/compiler.jar --flagfile closure-compiler/closure.conf",
    "rollup-es6": "ngc -p src/tsconfig-es6.json && rollup -c rollup/rollup-es6.js && tsc dist/bundle-rollup-es6.js --target es5 --allowJS --outFile dist/bundle-rollup-es5.js && uglifyjs dist/bundle-rollup-es5.js -o dist/bundle-rollup-es5.min.js -c -m",
    "rollup-demo": "ngc -p src/tsconfig-es5.json && rollup -c rollup/rollup-config-demo.js",
    "rollup-lazy": "ngc -p src/tsconfig-es5-lazy.json && rollup -c rollup/rollup-lazy-config.js",
    "systemjs-demo": "tsc -p src/tsconfig-es5-commonjs.json",
    "parcel-demo": "ngc -p src/tsconfig-es6.json && parcel build built-es6/src/apps/bundler-comparison-app/main.js --out-dir dist/parcel-bundle",
    "systemjs-builder-demo": "npm run systemjs-demo && node systemjs-builder/systemjs-builder.js",
    "webpack-no-lazy-loading": "ngc -p src/tsconfig-es5.json && webpack --config webpack/webpack-no-lazy-loading.js --progress --mode production",
    "webpack-lazy-loading": "webpack --config webpack/webpack-lazy-loading.js --progress --mode production",
    "webpack-no-angular-optimizations": "ngc -p src/tsconfig-es5.json && webpack --config webpack/webpack-no-angular-optimizations.js --progress --mode production",
    "webpack-babili": "ngc -p src/tsconfig-es6.json && webpack --config webpack/webpack-babili.js --progress --mode production",
    "build-all": "npm run build-demo-app && npm run aot-i18n-en && npm run aot-i18n-no && npm run rollup-lazy && npm run browserify-demo && npm run parcel-demo && npm run webpack-babili && npm run webpack-lazy-loading && npm run webpack-no-lazy-loading && npm run closure-compiler-demo && npm run rollup-demo && npm run rollup-es6",
    "postinstall": "ngc -p angular.tsconfig.json",
    "serve-app": "ibazel run //src/apps/main-demo-app:devserver",
    "serve-api": "ibazel run //src/apps/api",
    "start": "concurrently \"npm run serve-app\" \"npm run serve-api\""
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "0.3.1",
    "@ngtools/webpack": "6.0.8",
    "@angular/bazel": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/compiler-cli": "6.0.4",
    "@bazel/ibazel": "0.4.0",
    "babili-webpack-plugin": "^0.1.1",
    "browserify": "14.5.0",
    "parcel-bundler": "1.4.0",
    "path": "0.12.7",
    "raw-loader": "0.5.1",
    "systemjs-builder": "0.16.4",
    "typescript": "2.7.2",
    "uglify-js": "3.2.2",
    "webpack": "4.8.3",
    "webpack-cli": "2.1.3",
    "concurrently": "3.5.1",
    "compression": "1.7.2"
  },
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/common": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/platform-server": "6.0.4",
    "@angular/router": "6.0.4",
    "@angular/upgrade": "6.0.4",
    "angular": "~1.4.7",
    "bootstrap": "~3.3.4",
    "core-js": "^2.4.1",
    "events": "^1.0.2",
    "fbjs": "^0.3.1",
    "flux": "^2.1.1",
    "google-closure-compiler": "20180319.0.0",
    "jquery": "3.3.1",
    "jquery-ui-dist": "1.12.1",
    "react": "~0.13.3",
    "rollup": "0.55.0",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1",
    "rxjs": "6.2.1",
    "systemjs": "0.20.19",
    "underscore": "~1.8.3",
    "zone.js": "0.8.26",
    "express": "4.16.3"
  }
}
