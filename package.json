{
  "name": "angular-samples",
  "scripts": {
    "postinstall": "ngc -p angular.tsconfig.json",
    "start-remote-cache": "docker-compose -f caching/docker-compose.yml up",
    "stop-remote-cache": "docker-compose -f caching/docker-compose.yml down",
    "create-vendor-bundle": "webpack --config vendor/webpack-config.js",
    "build": "bazel build //src/apps/demo-app:devserver && bazel build //src/apps/api && bazel build //src/apps/backend",
    "start": "npm run build && concurrently \"ibazel run //src/apps/demo-app:devserver\" \"ibazel run //src/apps/api\" \"ibazel run //src/apps/backend\""
  },
  "devDependencies": {
    "@angular/bazel": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/compiler-cli": "6.0.4",
    "@bazel/ibazel": "0.4.0",
    "compression": "1.7.2",
    "concurrently": "3.5.1",
    "path": "0.12.7",
    "typescript": "2.7.2",
    "webpack": "4.14.0",
    "webpack-cli": "3.0.8"
  },
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/common": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/platform-server": "6.0.4",
    "@angular/router": "6.0.4",
    "@angular/upgrade": "6.0.4",
    "angular": "~1.4.7",
    "bootstrap": "~3.3.4",
    "core-js": "^2.4.1",
    "events": "^1.0.2",
    "express": "4.16.3",
    "fbjs": "^0.3.1",
    "ffi": "2.2.0",
    "flux": "^2.1.1",
    "jquery": "3.3.1",
    "jquery-ui-dist": "1.12.1",
    "react": "~0.13.3",
    "request": "2.87.0",
    "rxjs": "6.2.1",
    "systemjs": "0.20.19",
    "underscore": "~1.8.3",
    "zone.js": "0.8.26"
  }
}
