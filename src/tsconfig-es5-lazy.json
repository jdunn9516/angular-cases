{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": ["es2015", "dom"],
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "outDir": "../built-es5-lazy"
  },
  "files": [
    "apps/lazy-loading-app/app.module.ts",
    "apps/lazy-loading-app/lazy-modules/dynamic-form.module.ts",
    "apps/lazy-loading-app/lazy-modules/graph.module.ts",
    "apps/lazy-loading-app/lazy-modules/input-controls.module.ts",
    "apps/lazy-loading-app/lazy-modules/insertion-sort.module.ts",
    "apps/lazy-loading-app/lazy-modules/lazy-loaded-treeview.module.ts",
    "apps/lazy-loading-app/lazy-modules/pub-sub.module.ts",
    "apps/lazy-loading-app/lazy-modules/redux.module.ts",
    "apps/lazy-loading-app/lazy-modules/rxjs-buffering.module.ts",
    "apps/lazy-loading-app/lazy-modules/rxjs-streams.module.ts",
    "apps/lazy-loading-app/lazy-modules/spreadsheet.module.ts",
    "apps/lazy-loading-app/lazy-modules/text-editor.module.ts",
    "apps/lazy-loading-app/main.ts"
  ],

  "angularCompilerOptions": {
    "annotateForClosureCompiler": false,
    "annotationsAs": "static fields",
    "preserveWhitespaces": false
  }
}